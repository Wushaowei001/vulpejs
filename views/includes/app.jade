- var modules="";
if page.editor
  - modules += ", 'textAngular'";
if page.minicolors
  - modules += ", 'minicolors'";
if page.switch
  - modules += ", 'toggle-switch'";
if page.uploader
  - modules += ", 'blueimp.fileupload'";
if page.tree
  - modules += ", 'ui.tree'";
script(type="text/javascript").
  application.ng.base = angular.module('app', ['app.controllers'#{modules}]);
